{
  "name": "club-management-system",
  "version": "1.0.0",
  "description": "Membership and facilities management system for members-only club",
  "main": "index.js",
  "workspaces": [
    "server",
    "client",
    "tests"
  ],
  "packageManager": "npm@10.5.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:server": "turbo run dev --filter=club-management-server",
    "dev:client": "turbo run dev --filter=club-management-client",
    "dev:tests": "turbo run test:watch --filter=club-management-tests",
    "dev:simple": "turbo run dev --filter=club-management-server --filter=club-management-client --parallel",
    "setup": "npm install",
    "build": "turbo run build",
    "build:server": "turbo run build --filter=club-management-server",
    "build:client": "turbo run build --filter=club-management-client",
    "start": "turbo run start --filter=club-management-server",
    "test": "turbo run test",
    "test:server": "turbo run test --filter=club-management-server",
    "test:client": "turbo run test --filter=club-management-client",
    "test:e2e": "turbo run test --filter=club-management-tests",
    "test:watch": "turbo run test:watch --parallel",
    "lint": "turbo run lint",
    "lint:server": "turbo run lint --filter=club-management-server",
    "lint:client": "turbo run lint --filter=club-management-client",
    "typecheck": "turbo run typecheck",
    "typecheck:server": "turbo run typecheck --filter=club-management-server",
    "typecheck:client": "turbo run typecheck --filter=club-management-client",
    "deadcode": "knip",
    "deadcode:production": "knip --production",
    "clean": "turbo run clean && rm -rf node_modules",
    "reset": "npm run clean && npm run setup",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down"
  },
  "keywords": [
    "club",
    "membership",
    "management",
    "events",
    "facilities"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "knip": "^5.62.0",
    "turbo": "^2.5.5"
  }
}
