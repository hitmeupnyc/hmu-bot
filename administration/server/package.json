{
  "name": "club-management-server",
  "version": "1.0.0",
  "description": "Backend API for club management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules coverage",
    "db:migrate": "kysely migrate",
    "db:migrate:up": "kysely migrate up",
    "db:migrate:down": "kysely migrate down",
    "db:migrate:latest": "kysely migrate latest",
    "db:migrate:list": "kysely migrate list",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:setup-test": "tsx src/scripts/setup-test-db.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.2.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "discord.js": "^14.21.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "klaviyo-api": "^19.0.0",
    "kysely": "^0.28.3",
    "morgan": "^1.10.0",
    "patreon": "^0.4.1",
    "redis": "^4.6.12",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "kysely-ctl": "^0.14.0",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
