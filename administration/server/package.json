{
  "name": "api-server",
  "version": "1.0.0",
  "description": "Backend API for club management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:e2e": "NODE_ENV=test DATABASE_PATH=./data/test.db tsx watch src/index.ts",
    "build": "tsc",
    "build:sdk": "tsc --project tsconfig.sdk.json",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "kysely migrate",
    "db:migrate:latest": "mkdir -p ./data && kysely migrate latest",
    "db:seed": "kysely seed run",
    "db:setup-test": "rm -f ./data/test.db && mkdir -p ./data && DATABASE_PATH=./data/test.db NODE_ENV=test npm run db:migrate:latest && DATABASE_PATH=./data/test.db NODE_ENV=test npm run db:seed",
    "db:codegen": "kysely-codegen --dialect sqlite --url ./data/club.db --out-file ./src/types/index.ts",
    "auth:init": "tsx src/scripts/init-spicedb.ts",
    "auth:init:prod": "tsx src/scripts/init-spicedb-production.ts",
    "auth:test": "vitest --run src/__tests__/Authorization.test.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@effect/opentelemetry": "^0.56.4",
    "@effect/platform": "^0.90.6",
    "@effect/platform-node": "^0.96.0",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "better-auth": "^1.3.5",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^16.3.1",
    "effect": "^3.17.4",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^7.1.0",
    "klaviyo-api": "^19.0.0",
    "kysely": "^0.28.3",
    "morgan": "^1.10.0",
    "patreon": "^0.4.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@effect/language-service": "^0.33.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "kysely-codegen": "^0.18.5",
    "kysely-ctl": "^0.14.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./sdk": {
      "types": "./dist-sdk/sdk/index.d.ts",
      "default": "./dist-sdk/sdk/index.js"
    },
    "./sdk/effect": {
      "types": "./dist-sdk/sdk/client.d.ts", 
      "default": "./dist-sdk/sdk/client.js"
    },
    "./sdk/promise": {
      "types": "./dist-sdk/sdk/promise.d.ts",
      "default": "./dist-sdk/sdk/promise.js"
    },
    "./sdk/types": {
      "types": "./dist-sdk/sdk/types.d.ts"
    }
  },
  "overrides": {
    "better-sqlite3": "^12.2.0"
  }
}
