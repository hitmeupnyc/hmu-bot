{
  "name": "api-server",
  "version": "1.0.0",
  "description": "Backend API for club management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules coverage",
    "db:migrate": "kysely migrate",
    "db:migrate:up": "kysely migrate up",
    "db:migrate:down": "kysely migrate down",
    "db:migrate:latest": "mkdir -p data && kysely migrate latest",
    "db:migrate:list": "kysely migrate list",
    "db:seed": "kysely seed run",
    "db:setup-test": "tsx src/scripts/setup-test-db.ts",
    "db:codegen": "kysely-codegen --dialect sqlite --url ./data/club.db --out-file ./src/types/database.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^16.3.1",
    "effect": "^3.17.4",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^7.1.0",
    "klaviyo-api": "^19.0.0",
    "kysely": "^0.28.3",
    "morgan": "^1.10.0",
    "patreon": "^0.4.1",
    "redis": "^4.6.12",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@effect/language-service": "^0.33.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "kysely-codegen": "^0.18.5",
    "kysely-ctl": "^0.14.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "better-sqlite3": "^12.2.0"
  }
}
